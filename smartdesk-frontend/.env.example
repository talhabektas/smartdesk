# SmartDesk Frontend Environment Variables
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:8067/api/v1
REACT_APP_WS_URL=http://localhost:8067/ws

# App Configuration
REACT_APP_APP_NAME=SmartDesk CRM
REACT_APP_APP_VERSION=1.0.0

# Development Configuration
REACT_APP_ENABLE_MOCK_DATA=true
REACT_APP_LOG_LEVEL=debug

# Feature Flags
REACT_APP_FEATURE_DARK_MODE=true
REACT_APP_FEATURE_NOTIFICATIONS=true
REACT_APP_FEATURE_ANALYTICS=true

# External Services (for future use)
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here